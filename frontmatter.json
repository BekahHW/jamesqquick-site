{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.startCommand": "npm run dev",
  "frontMatter.preview.host": "localhost:3000",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "slug",
          "name": "slug",
          "type": "slug"
        },
        {
          "title": "coverImage",
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "youTubeVideoId",
          "name": "youTubeVideoId",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags",
          "taxonomyLimit": 3
        }
      ]
    },
    {
      "name": "Testimonial",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Fullname",
          "name": "name",
          "type": "string",
          "single": true
        },
        {
          "title": "Image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        }
      ]
    },
    {
      "name": "Course",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug"
        },
        {
          "title": "Publishing date",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/src/content/blog",
      "title": "blog",
      "originalPath": "[[workspace]]/src/content/blog",
      "contentTypes": [
        "default"
      ]
    },
    {
      "path": "[[workspace]]/src/content/course",
      "title": "course",
      "originalPath": "[[workspace]]/src/content/course",
      "contentTypes": [
        "Course"
      ]
    },
    {
      "title": "testimonial",
      "path": "[[workspace]]/src/content/testimonial",
      "contentTypes": [
        "Testimonial"
      ]
    }
  ]
}